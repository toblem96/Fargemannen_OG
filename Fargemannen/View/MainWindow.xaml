<Window x:Class="Fargemannen.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Fargemannen.View"
        xmlns:vm="clr-namespace:Fargemannen.ViewModel"
        
        Title="Fargemannen" Height="700" Width="700">

    <Grid>

        <TabControl>
            <TabItem Header="FilOpplasting" Width="150" HorizontalContentAlignment="Center">
                <local:FileUpload />
            </TabItem>
            <TabItem Header="Symboler" Width="150" HorizontalContentAlignment="Center">
                <local:Symbols />
            </TabItem>
            <TabItem Header="AnalyseXY" Width="150" HorizontalContentAlignment="Center">
                <local:AnalyseXY />
            </TabItem>
            <TabItem Header="AnalyseZ" Width="150" HorizontalContentAlignment="Center">
                <local:AnalyseZ />
            </TabItem>
            <TabItem Header="DIV" Width="50" HorizontalContentAlignment="Center">
                <local:DIV />
            </TabItem>

        </TabControl>
    </Grid>
</Window>
